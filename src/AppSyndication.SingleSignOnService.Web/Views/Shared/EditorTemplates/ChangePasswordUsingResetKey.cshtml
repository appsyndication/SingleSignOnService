@model AppSyndication.AccountService.Web.Models.ChangePasswordUsingResetKey

<div class="form-group">
    <label for="Password">Password</label>
    <input id="Password" name="Password" type="password" class="form-control" required maxlength="100" autocomplete="off" autofocus/>
</div>
<div class="form-group">
    <label for="ConfirmPassword">Confirm Password</label>
    <input id="ConfirmPassword" name="ConfirmPassword" type="password" class="form-control" required maxlength="100" autocomplete="off"/>
</div>
<input name="Key" type="hidden" value="@Model.Key">
